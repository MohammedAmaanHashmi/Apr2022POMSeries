            steps{
                echo("Run regression tests on QA")
            }
        }
        stage("Deploy on STAGE"){
            steps{
                echo("Deploy on STAGE")
            }
        }
            
            
            stage("Deploy on PROD"){
            steps{
                echo("Deploy on PROD")
            }
        }
        }
        
